{
  "author": "Turo engineering",
  "description": "GitHub Action for Semantic Release",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@vercel/ncc": "^0.38.1",
    "semver": "^7.6.2"
  },
  "devDependencies": {
    "@jest/globals": "30.1.2",
    "@open-turo/eslint-config-typescript": "18.0.8",
    "@types/jest": "30.0.0",
    "@types/node": "22.18.1",
    "eslint": "9.34.0",
    "jest": "30.1.3",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "semantic-release": "24.2.7",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "ts-patch": "3.3.0",
    "tsc-watch": "7.1.1",
    "typescript": "5.9.2",
    "typescript-transform-paths": "3.5.5"
  },
  "engines": {
    "node": ">= 20"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "name": "@open-turo/semantic-release-action",
  "peerDependencies": {
    "semantic-release": ">=23.0.0"
  },
  "private": true,
  "scripts": {
    "build": "ncc build src/index.ts -m -e semantic-release -o dist --source-map --license licenses.txt",
    "clean": "rimraf dist",
    "lint": "eslint",
    "test": "node --experimental-vm-modules ../node_modules/jest/bin/jest.js",
    "watch": "clean && tsc-watch -w -p ./tsconfig.build.json --compiler ./node_modules/.bin/tspc"
  },
  "type": "module",
  "types": "lib/index.d.ts"
}
