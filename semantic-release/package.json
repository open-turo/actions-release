{
  "author": "Turo engineering",
  "description": "GitHub Action for Semantic Release",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@vercel/ncc": "^0.36.1",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@jest/globals": "^29.6.4",
    "@open-turo/eslint-config-typescript": "4.0.0",
    "@types/jest": "29.5.4",
    "@types/node": "18.16.19",
    "@types/semantic-release": "^20.0.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-typescript-sort-keys": "2.3.0",
    "eslint-plugin-unicorn": "48.0.0",
    "jest": "29.6.4",
    "prettier": "3.0.0",
    "prettier-2": "npm:prettier@^2",
    "rimraf": "5.0.1",
    "semantic-release": "22.0.12",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-patch": "3.0.1",
    "tsc-watch": "6.0.4",
    "typescript": "5.1.6",
    "typescript-transform-paths": "3.4.6"
  },
  "engines": {
    "node": ">= 20"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "name": "@open-turo/semantic-release-action",
  "peerDependencies": {
    "semantic-release": ">=20.1.0"
  },
  "scripts": {
    "build": "ncc build src/index.ts -m -e semantic-release -o dist --source-map --license licenses.txt",
    "clean": "rimraf dist",
    "lint": "eslint ./src ./test --ext .ts",
    "test": "node --experimental-vm-modules ../node_modules/jest/bin/jest.js",
    "watch": "clean && tsc-watch -w -p ./tsconfig.build.json --compiler ./node_modules/.bin/tspc"
  },
  "type": "module",
  "types": "lib/index.d.ts"
}
